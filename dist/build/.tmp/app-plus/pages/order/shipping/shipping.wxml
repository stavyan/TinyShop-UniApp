<view><block wx:for="{{shippingDetail.data}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view class="goods-section"><view class="g-header b-b"><text class="name">商品信息</text></view><view class="g-item"><image src="{{item.order_product.product_picture}}" mode="aspectFill"></image><view class="right"><text class="title clamp">{{item.order_product.product_name}}</text></view></view></view><view class="yt-list"><block wx:if="{{item.member_username}}"><view class="yt-list-cell b-b"><view class="cell-icon">名</view><text class="cell-tit clamp">收件人</text><text class="cell-tip active">{{''+item.member_username+''}}</text><text class="cell-more wanjia wanjia-gengduo-d"></text></view></block><block wx:if="{{item.express_no}}"><view class="yt-list-cell b-b"><view class="cell-icon">单</view><text class="cell-tit clamp">快递单号</text><text class="cell-tip active">{{''+item.express_no+''}}</text><text class="cell-more wanjia wanjia-gengduo-d"></text></view></block><block wx:if="{{item.express_company}}"><view class="yt-list-cell b-b"><view class="cell-icon">司</view><text class="cell-tit clamp">快递公司</text><text class="cell-tip active">{{''+item.express_company+''}}</text><text class="cell-more wanjia wanjia-gengduo-d"></text></view></block></view><view class="uni-timeline" style="padding:30rpx 40rpx;background-color:#fff;"><block wx:for="{{item.trace}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="{{['uni-timeline-item',(index===0)?'uni-timeline-first-item':'',(index===item.trace.length-1)?'uni-timeline-last-item':'']}}"><view class="uni-timeline-item-divider"></view><view class="uni-timeline-item-content"><view>{{''+row.value+''}}</view><block wx:if="{{row.trace.length===0}}"><view class="datetime">{{''+row.time+''}}</view></block></view></view></block><view>暂无物流信息</view></view></view></block><block wx:if="{{shippingDetail.count===0}}"><rf-empty vue-id="1" info="暂无物流信息" bind:__l="__l"></rf-empty></block></view>